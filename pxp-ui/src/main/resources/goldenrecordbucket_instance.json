{
	"dynamic": "true",
    "properties": {
            "organisationCode": {
				"type": "keyword"
			},
			"endpointCode": {
				"type": "keyword"
			},
			"catalogCode": {
				"type": "keyword"
			},
			"ruleId": {
				"type": "keyword"
			},
			"linkedBaseEntites": {
				"type": "long"
			},
			"isSearchable": {
				"type": "boolean"
			},
			"createdTime": {
				"type": "long"
			},
			"lastModifiedTime": {
				"type": "long"
			},
			"propertyObjects": {
				"type": "nested",
				"properties": {
					"attribute": {
					"type": "object",
					"properties": {
						"independent": {
							"type": "object"
							}
						}
					},
					"tag": {
						"type": "object"
					}
				}
			}
		},
		"dynamic_templates": [
		{
          "long_template": {
            "path_match": "propertyObjects.attribute.*.i_*",
            "mapping": {
              "fields": {
                "raw": {
                  "type": "keyword"
                }
              },
              "type": "double"
            }
          }
        },
		{
			"text_template": {
				"path_match": "propertyObjects.attribute.*.t_*",
				"mapping": {
					"type": "text",
					"analyzer":"searchable",
					"fields": {
						"raw": {
							"type": "keyword"
						},
						"rawlowercase":{
							"type": "keyword",
							"normalizer":"lowercase"
						}
					}
				}
			}
		},
		{
			"tag_template": {
				"path_match": "propertyObjects.tag.*",
				"mapping": {
					"type": "keyword"
				}
			}
		}
	]
}