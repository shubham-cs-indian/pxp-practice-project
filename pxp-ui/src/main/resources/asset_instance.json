{
	"dynamic": "true",
	"properties": {
		"id": {
			"type": "keyword"
		},
		"baseEntityIid": {
			"type": "long"
		},
		"contextID": {
			"type": "keyword"
		},
		"contextStartTime": {
			"type": "long"
		},
		"contextEndTime": {
			"type": "long"
		},
		"baseentityid": {
			"type": "keyword"
		},
		"classIds": {
			"type": "keyword"
		},
		"taxonomyIds": {
			"type": "keyword"
		},
		"catalogCode": {
			"type": "keyword"
		},
		"organisationCode": {
			"type": "keyword"
		},
		"endpointCode": {
			"type": "keyword"
		},
		"baseType": {
			"type": "keyword"
		},
		"parentIID": {
			"type": "long"
		},
		"topParentIID": {
			"type": "long"
		},
		"collectionIds":{
			"type": "keyword"
		},
		"isRoot": {
			"type": "boolean"
		},
		"red":{
			"type": "keyword"
		},
		"orange":{
			"type": "keyword"
		},
		"yellow":{
			"type": "keyword"
		},
		"propertyObjects": {
			"type": "nested",
			"properties": {
				"identifier": {
					"type": "keyword"
				},
				"attribute": {
					"type": "object",
					"properties": {
						"independent": {
							"type": "object"
						}
					}
				},
				"tag": {
					"type": "object"
				}
			}
		},
		"isExpired": {
			"type": "boolean"
		},
		"isDuplicate": {
			"type": "boolean"
		}
	},
	"dynamic_templates": [
		{
          "long_template": {
            "path_match": "propertyObjects.attribute.*.i_*",
            "mapping": {
              "fields": {
                "raw": {
                  "type": "keyword"
                }
              },
              "type": "double"
            }
          }
        },
		{
			"text_template": {
				"path_match": "propertyObjects.attribute.*.t_*",
				"mapping": {
					"type": "text",
					"analyzer":"searchable",
					"fields": {
						"raw": {
							"type": "keyword"
						},
						"rawlowercase":{
							"type": "keyword",
							"normalizer":"lowercase"
						}
					}
				}
			}
		},
		{
			"tag_template": {
				"path_match": "propertyObjects.tag.*",
				"mapping": {
					"type": "keyword"
				}
			}
		}
	]
}